{
    "site_name": "py-azure-web-app",
    "python_version": ["3.5.2", "2.7.12"],
    "cpu_arch": ["x64", "x86"],
    "site_type": ["socket", "WSGI"],
    "socket_port_env_var":
        "{% if cookiecutter.site_type == 'socket' %}PORT{% else %}N/A{% endif %}",
    "socket_python_arguments":
        "{% if cookiecutter.site_type == 'socket' %}D:\\home\\site\\wwwroot\\runserver.py --port %HTTP_PLATFORM_PORT%{% else %}N/A{% endif %}",
    "wsgi_app_object":
        "{% if cookiecutter.site_type == 'WSGI' %}app.wsgi_app{% else %}N/A{% endif %}",
    "log_file_path": "D:\\home\\LogFiles\\python.log"
}
